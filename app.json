{
  "expo": {
    "name": "finalProject_coin-collector",
    "slug": "finalProject_coin-collector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "be.thebelgianeagles.coincollector"
    },
    "web": {
      "favicon": "./assets/icon.jpg"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app wants to access your pictures"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4e8437a5-4f84-48b5-9a9f-b99571f98eb6"
      }
    }
  }
}
